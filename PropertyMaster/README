        echo "Select an option..\n\n";
        echo "    1) Do this\n";
        echo "    2) Do this\n";
        echo "    3) Do this\n";
        echo "    x) Exit\n";

        // Decide what menu option to select based on input..
        switch(trim(fgets(STDIN,256)))


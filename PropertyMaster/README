	foreach($argv as $val) {
		switch($val) {
		case "-propInitialSpace":
			$outputSpaceAfterProperty = 1;
			break;
		case "-propSpaces":
			$outputPropertySpaces = 0;
			break;
		}
	}



// Stop the script giving time out errors..
    set_time_limit(0);

    // This opens standard in ready for interactive input..
    define('STDIN',fopen("php://stdin","r"));

    // Main event loop to capture top level command..
    while(!0)

function show_home_directory() {
    # works from CLI and in web browser:
    echo getenv("HOME"). "<br>\n";

    # only via http
    global $HTTP_ENV_VARS;
    echo $HTTP_ENV_VARS["HOME"] . "<br>\n";
  }

  show_home_directory();
